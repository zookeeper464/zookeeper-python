N=int(input())
lst=input().split()

down=[]
up=[]
lst1=[]
lst2=[]
for i in range(N+1):
  down.append(str(9-i))
  up.append(str(i))
for i in range(N):
  if lst[i]=="<":
    lst1.append(i)
  if lst[i]==">":
    lst2.append(i)

def check (ls,lt,n):
  for i in range(len(lt)-n):
    if lt[i]+n==lt[i+n]:
      ls[lt[i]],ls[lt[i]+1]=ls[lt[i]+1],ls[lt[i]]

for i in range(len(lst1)):
  check(down,lst1,i)

for i in range(len(lst2)):
  check(up,lst2,i)

down="".join(down)
up="".join(up)
print(down)
print(up)
